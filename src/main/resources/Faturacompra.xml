<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="entity.Faturacompra">
        <table name="faturacompra" schema="public" catalog="Proj2"/>
        <attributes>
            <id name="idfaturac">
                <column name="idfaturac"/>
            </id>
            <basic name="idencomenda">
                <column name="idencomenda"/>
            </basic>
            <basic name="idtipopag">
                <column name="idtipopag"/>
            </basic>
            <basic name="valor">
                <column name="valor"/>
            </basic>
            <basic name="quantidade">
                <column name="quantidade"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
