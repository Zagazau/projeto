<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="entity.Faturavenda">
        <table name="faturavenda" schema="public" catalog="Proj2"/>
        <attributes>
            <id name="idfaturav">
                <column name="idfaturav"/>
            </id>
            <basic name="idpedido">
                <column name="idpedido"/>
            </basic>
            <basic name="idtipopag">
                <column name="idtipopag"/>
            </basic>
            <basic name="idcliente">
                <column name="idcliente"/>
            </basic>
            <basic name="valor">
                <column name="valor"/>
            </basic>
            <basic name="quantidade">
                <column name="quantidade"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
