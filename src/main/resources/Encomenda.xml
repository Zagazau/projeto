<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="entity.Encomenda">
        <table name="encomenda" schema="public" catalog="Proj2"/>
        <attributes>
            <id name="idencomenda">
                <column name="idencomenda"/>
            </id>
            <basic name="idfornecedor">
                <column name="idfornecedor"/>
            </basic>
            <basic name="quantidade">
                <column name="quantidade"/>
            </basic>
            <basic name="data">
                <column name="data"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
