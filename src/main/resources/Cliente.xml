<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="entity.Cliente">
        <table name="cliente" schema="public" catalog="Proj2"/>
        <attributes>
            <id name="idcliente">
                <column name="idcliente"/>
            </id>
            <basic name="codpostal">
                <column name="codpostal"/>
            </basic>
            <basic name="nome">
                <column name="nome"/>
            </basic>
            <basic name="telefone">
                <column name="telefone"/>
            </basic>
            <basic name="rua">
                <column name="rua"/>
            </basic>
            <basic name="numporta">
                <column name="numporta"/>
            </basic>
            <basic name="nif">
                <column name="nif"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
