<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="entity.Admin">
        <table name="admin" schema="public" catalog="Proj2"/>
        <attributes>
            <id name="id">
                <column name="id"/>
            </id>
            <basic name="username">
                <column name="username"/>
            </basic>
            <basic name="senha">
                <column name="senha"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
